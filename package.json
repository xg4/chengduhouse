{
  "name": "chengduhouse",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/xg4/chengduhouse#readme",
  "bugs": {
    "url": "https://github.com/xg4/chengduhouse/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg4/chengduhouse.git"
  },
  "license": "MIT",
  "author": "xg4 <xingor4@gmail.com>",
  "scripts": {
    "prebuild": "rimraf dist && npm run db:migrate && npm run db:gen",
    "build": "nest build",
    "db:migrate": "prisma migrate deploy",
    "db:gen": "prisma generate",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "node dist/src/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "2.1.0",
    "@nestjs/core": "^8.4.6",
    "@nestjs/jwt": "8.0.1",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/schedule": "2.0.1",
    "@prisma/client": "3.15.1",
    "bcrypt": "5.0.1",
    "cheerio": "1.0.0-rc.11",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.3",
    "lodash": "4.17.21",
    "passport": "0.5.3",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "undici": "5.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.6",
    "@types/bcrypt": "5.0.0",
    "@types/cron": "2.0.0",
    "@types/crypto-js": "4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.1",
    "@types/lodash": "4.14.182",
    "@types/node": "^17.0.42",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.1",
    "prettier": "^2.6.2",
    "prisma": "3.15.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
